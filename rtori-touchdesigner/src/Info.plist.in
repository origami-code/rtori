<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- The order will follow the official doc from apple -->

    <!-- Categorization -->
    <key>CFBundlePackageType</key>
	<string>BNDL</string>

    <!-- Identification -->
	<key>CFBundleIdentifier</key>
	<string>${IDENTIFIER}</string>

    <!-- Naming -->
    <key>CFBundleName</key>
    <string>${NAME_SHORT}</string>
    <key>CFBundleDisplayName</key>
    <string>${NAME_FULL}</string>

    <!-- Versioning -->
    <key>CFBundleShortVersionString</key>
    <string>${VERSION_TRIPLET}</string>
    <key>CFBundleVersion</key>
    <string>${VERSION_TRIPLET_WITH_BUILD}</string>
	<key>CFBundleLongVersionString</key>
	<string>${VERSION_HUMAN}</string>
	<key>NSHumanReadableCopyright</key>
	<string>${COPYRIGHT_HUMAN}</string>

    <!-- Localization -->
    <key>CFBundleDevelopmentRegion</key>
    <string>English</string>

    <!-- Others -->
	<key>CFBundleExecutable</key>
	<string>${EXECUTABLE_NAME}</string> <!-- The binary that is loaded -->
	<key>CFBundleIconFile</key>
	<string>${ICON_FILE}</string>

    <!-- Misc -->
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
    <key>CFBundleSignature</key>
	<string>????</string> <!-- On purpose -->
</dict>
</plist>
